{
  "name": "cv-stalin-nunes",
  "version": "2.6.0",
  "private": true,
  "description": "Currículo multilíngue com tradução IA, controle de tokens e exportação PDF.",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:tp": "next dev --turbopack",
    "build": "next build",
    "clean": "rimraf .next",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check \"src/styles/**/*.css\"",
  "format:write": "prettier --write \"src/styles/**/*.css\"",
  "gitflow": "bash ./automation/git-flow.sh",
  "docs:ecom": "node ./scripts/concat-ecom-docs.mjs",
  "previews:install": "playwright install --with-deps",
  "previews:gen": "node ./scripts/generate-previews.mjs",
  "previews:dev": "start-server-and-test \"yarn dev\" http://localhost:3000 \"yarn previews:gen\"",
  "previews:cv": "node ./scripts/generate-previews.mjs --only=cv --quality=88 --delay=1100",
  "previews:cv:dev": "start-server-and-test \"yarn dev\" http://localhost:3000 \"yarn previews:cv\"",
  "export:cv": "node ./scripts/export-app.mjs --app=cv --out=exports --zip",
  "export:motd": "node ./scripts/export-app.mjs --app=motd --out=exports --zip",
  "export:ecommerce": "node ./scripts/export-app.mjs --app=ecommerce --out=exports --zip",
  "export:all": "node ./scripts/export-app.mjs --app=cv --out=exports --zip && node ./scripts/export-app.mjs --app=motd --out=exports --zip && node ./scripts/export-app.mjs --app=ecommerce --out=exports --zip"
  },
  "dependencies": {
    "googleapis": "^150.0.1",
    "next": "15.3.3",
    "openai": "^5.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
  "prettier": "^3.3.3",
  "playwright": "^1.46.1",
  "start-server-and-test": "^2.0.3",
  "ts-node": "^10.9.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "browserslist": [
    "since 2020",
    "not dead"
  ]
}
